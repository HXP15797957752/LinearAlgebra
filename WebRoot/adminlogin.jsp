</head>
<body>
	
<script>$(document).ready(function(c) {
	$('.close').on('click', function(c){
		$('.login-form').fadeOut('slow', function(c){
	  		$('.login-form').remove();
		});
	});	  
});
</script>
 <!--SIGN UP-->
 <h1>后台登录</h1>
<div class="login-form">
	<div class="close"> </div>
		<div class="head-info">
			<label class="lbl-1"> </label>
			<label class="lbl-2"> </label>
			<label class="lbl-3"> </label>
		</div>
			<div class="clear"> </div>
	<div class="avtar">
		<img src="images/avtar.png" />
	</div>
			<form action="<c:url value='/AdminLoginServlet'/>" method="post">
						<p style="color: red; font-size: 11pt; font-weight: 900;">${msg}</p>
					<input type="text" class="text" name="id" value="${form.id }" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '${form.id }';}" >
						<div class="key">
					<input type="password" name="password" value="${form.password }" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '${form.password }';}">
						</div>
			 
					<div class="signin">
						<input type="submit" value="登录" >
					</div>
			</form>
</div>

</body>
</html>